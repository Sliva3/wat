unit wfiglet {
    figlet_reflection := __refl__.reflect(
        'com.kilowatt.Compiler.Builtins.Libraries.Utils.UtilsFiglet',
        []
    )

    fun art(font_path, text) {
        return figlet_reflection.art(font_path, text)
    }

    fun println(font_path, text) {
        figlet_reflection.println(font_path, text)
    }
}