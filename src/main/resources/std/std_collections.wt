type Stack {
    stack := []

    fun push(val) {
        stack.add(val)
    }

    fun pop() {
        index := stack.size() - 1
        val := stack.get(index)
        stack.delete_at(index)
        return val
    }

    fun size() {
        return stack.size()
    }

    fun top() {
        index := stack.size() - 1
        val := stack.get(index)
        return val
    }
}